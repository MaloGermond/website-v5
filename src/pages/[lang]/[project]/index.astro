export function getStaticPaths() {
  const projects = ["unistellar", "privowny"]

  const path = locales.flatMap((lang) =>
    projects.map((project) => ({
      params: { lang, project }, // Génère /fr/unistellar, /fr/privowny, /en/unistellar, etc.
    })))

  return path;
}
